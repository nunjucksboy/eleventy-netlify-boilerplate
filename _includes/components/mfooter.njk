<section class="m-footer">
     <div class="wrapper">
          <div class="box-footer">
               <h3>KATAGORI</h3>
               <ul>
               {% for post in postslist | reverse %}
               {% if post.data.tags %}
                    {% for tag in post.data.tags %}
                    <li> 
                         {% set tagUrl %}/tags/{{ tag }}/{% endset %}
					<a href="{{ tagUrl | url }}" rel="tag">{{ tag }}</a>
                    </li>
                    {% endfor %}
               {%- endif -%}
               {% endfor %}
               </ul>
          </div>
          <div class="box-footer">
               <h3>BLOG</h3>
