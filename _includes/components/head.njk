<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or metadata.title }}</title>
  <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/jQuery.mmenu/7.3.3/jquery.mmenu.all.css">
  <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js" type="text/javascript"></script>  
   <link rel="icon" type="image/png" href="{{ site.baseurl }}static/img/favicon.ico" />
  {% set css %}
    {% include "assets/css/inline.css" %}
  {% endset %}

  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %} 
    {% include "assets/js/responsiveslides.js" %} 
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>

</head>

